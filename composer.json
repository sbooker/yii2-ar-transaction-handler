{
  "name": "sbooker/yii2-ar-transaction-handler",
  "description": "Yii2 active record transaction handler implementation for sbooker/transaction-manager",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Sergey Knigin",
      "email": "sergey.knigin.dev@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^7.4 || ^8.0",
    "sbooker/transaction-manager": "1.1.*",
    "yiisoft/yii2": "2.0.*"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "autoload": {
    "psr-4": {
      "Sbooker\\TransactionManager\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\Sbooker\\TransactionManager\\Yii2ActiveRecord\\": "tests/"
    }
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5"
  }
}
